{% extends 'base.html.twig' %}

{% block title %}Hello VideoController!{% endblock %}

{% block body %}

<div class="container">
    {{ form_start(form) }}
    {{ form_row(form.title) }}
    {{ form_row(form.description) }}
    {{ form_row(form.url) }}
    {{ form_row(form.dateCreation) }}
    {{ form_row(form.iframe) }}
    {{ form_row(form.category) }}
    {{ form_row(form.user) }}
    {{ form_row(form.submit) }}
    {{ form_end(form) }}
</div>
{% endblock %}
